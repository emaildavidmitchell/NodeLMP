<% include header %>
	<div class="container">
		<form class="form" action="/network" method="get">
			<div class="form-group">
				<input class="form-control typeahead" placeholder="Search" name="search" id="search">
			</div>
			<button type="submit" class="btn btn-default">Submit</button>
	  	</form>
  </div>
	<script>
		var articles = <%- JSON.stringify(articles) %>;
		$('input.typeahead').typeahead({source: articles});
	</script>
<% include footer %>